{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ForceHttps": false,
  "IsRealmEnabled": true,
  "IdserverBaseUrl": "https://localhost:5001/sso/idserver",
  "ScimBaseUrl": "https://localhost:5003",
  "ClientId": "SIDS-manager",
  "ClientSecret": "password",
  "Scopes": [
    "openid",
    "profile",
    "role"
  ],
  "OpenTelemetryOptions": {
    "EnableOtpExported": true,
    "EnableConsoleExporter": true,
    "TracesEndpoint": "https://api.honeycomb.io/v1/traces",
    "Headers": "x-honeycomb-team=ExZLneG9DeipnZSuVFomXI",
    "Protocol": "HttpProtobuf"
  },
  "IgnoreCertificateError": true,
  "DetailedErrors": true
}
